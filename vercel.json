{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/hobbies1", "dest": "/hobbies1.html" },
    { "src": "/contact1", "dest": "/contact1.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
